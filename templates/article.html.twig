{% extends 'base.html.twig' %}
    {% block title %}
        <title>{{ list.title }}</title>
    {% endblock %}
    {% block main %}
        {% if list.isPublished %}
            <article class="pageversion">
               {% include '_article.html.twig' %}
                <p>{{ list.description }}</p>
                <img src="{{ list.image }}" alt="image">
            </article>
        {% else %}
            <p>Cet article n'est pas encore publi√©</p>
        {% endif %}
    {% endblock %}
    {% block footer %}
        <p>Je suis le footer de la page article solo</p>
    {% endblock %}